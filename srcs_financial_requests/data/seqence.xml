<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="payment_request_sequence" model="ir.sequence">
        <field name="name">Payment Request Sequence</field>
        <field name="code">payment.request</field>
        <field name="prefix">payment/%(year)s/%(month)s/</field>
        <field name="padding">4</field>
    </record>   
  
    <!-- Custody Clearance Sequence -->
    <record id="seq_finance_approval" model="ir.sequence">
        <field name="name">Custody Clearance No.</field>
        <field name="code">custody.clearance.sequence</field>
        <field name="prefix">CLR/%(year)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
        <field name="implementation">no_gap</field>
    </record>
    

</odoo>
