<?xml version="1.0" ?>
<odoo>
    <data noupdate='1'>

        <!--
            Sequence Type
        -->

        <record id="ir_seq_hr_loan" model="ir.sequence">
            <field name="name">Loan Request</field>
            <field name="code">hr.loan.req</field>
            <field name="prefix">Loan NO</field>
            <field name="padding">3</field>
        </record>

          <record id="loan_payment" model="ir.sequence">
            <field name="name">loan payment</field>
            <field name="code">loan.payment</field>
            <field name="prefix">/Payment/</field>
            <field name="padding">4</field>
            <field name="active">True</field>
            <field eval="1" name="number_increment"/>
        </record>

        <record id="loan_postpone" model="ir.sequence">
            <field name="name">loan postpone</field>
            <field name="code">loan.postpone</field>
            <field name="prefix">/Postpone/</field>
            <field name="padding">4</field>
            <field name="active">True</field>
            <field eval="1" name="number_increment"/>
        </record>

        <record id="loan_stop" model="ir.sequence">
            <field name="name">loan stop</field>
            <field name="code">loan.stop</field>
            <field name="prefix">/Stop/</field>
            <field name="padding">4</field>
            <field name="active">True</field>
            <field eval="1" name="number_increment"/>
        </record>

    </data>
</odoo>