<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--      loan car wizard form view-->
    <record id="loan_car_wiz_view" model="ir.ui.view">
        <field name="name">loan.car.wiz.new</field>
        <field name="model">loan.car.wiz</field>
        <field name="arch" type="xml">
            <form>
                <group col="4">
                    <field name="from_date"/>
                    <field name="to_date"/>
<!--                    <field name="car"/>-->
                    <!--                           attrs="{'invisible': [('filter','!=','test')] }"-->

                    <field name="loan_type"/>
                    <!--                           attrs="{'invisible': [('filter','!=','test2')] }"/>-->
                    <!--                    <field name="filter"/>-->
                </group>
                <footer>
                    <button name="print_report" string="Print" class="btn-primary" type="object"
                            data-hotkey="2"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" data-hotkey="2"/>
                </footer>
            </form>

        </field>
    </record>
</odoo>