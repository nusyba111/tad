<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--    total car move wizard form view-->
    <record id="total_car_move_wiz" model="ir.ui.view">
        <field name="name">total.car.move.new</field>
        <field name="model">total.car.move</field>
        <field name="arch" type="xml">
            <form>
                <group col="4">
                    <field name="from_date"/>
                    <field name="to_date"/>
                    <field name="car"/>
<!--                    <field name="number_of_days"/>-->
                </group>
                <footer>
                    <button name="print_report" string="Print" class="btn-primary" type="object"
                            data-hotkey="2"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" data-hotkey="2"/>
                </footer>
            </form>

        </field>
    </record>
</odoo>